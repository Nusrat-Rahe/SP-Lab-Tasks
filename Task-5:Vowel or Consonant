#include<stdio.h>
int main(){
    char c;
    scanf("%c",&c);
    char C=(c=='A'||c=='E'||c=='I'||c=='O'||c=='U'||c=='a'||c=='e'||c=='i'||c=='o'||c=='u');
switch(C){
       case 1:
        printf("vowel\n");
        break;
       default:
        printf("consonent\n");
        break;
        }
return 0;
}
