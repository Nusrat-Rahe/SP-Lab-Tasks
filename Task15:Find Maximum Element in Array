#include<stdio.h>
   int maximum(int arr[],int n){
   int max=arr[0];
   for(int i=0;i<n;i++){
    if(max<arr[i])
        max=arr[i];
   }
   return max;
   } 
    int main(){
    int arr[]={4,6,9,2,0};
    int size_arr=sizeof(arr)/sizeof(arr[0]);
    int max=maximum(arr,size_arr);
    printf("%d\n",max);
    return 0;
}
